                    x:Name="header"
                    Grid.Row="0"
                    Grid.Column="1"
                    HorizontalAlignment="Center">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" Foreground="Blue">
                        Učionica:
                    </TextBlock>
                    <TextBlock
                        x:Name="Oznaka_ucionica"
