    xmlns:common="clr-namespace:Common;assembly=RecentFileListLib"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Raspored"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="525"
    Height="350"
    local:HelpProvider.HelpKey="Forma"
    Activated="HandleWindowActivated"
    mc:Ignorable="d">
    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Help" Executed="CommandBinding_Executed" />
    </Window.CommandBindings>
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_Datoteka">
                <MenuItem Click="Button_Click" Header="_Novi" />
                <MenuItem Click="Button_Click_1" Header="_Otvori" />
                <common:RecentFileList x:Name="RecentFileList" />
                <MenuItem Click="MenuItem_Click_1" Header="_Zatvori" />
            </MenuItem>
            <MenuItem Header="_Prikaz">
                <MenuItem Click="Ucionice_Click" Header="_Ucionice" />
                <MenuItem Header="_Predmeta" />
                <MenuItem Header="Smera" />
                <MenuItem Header="Softvera" />
            </MenuItem>

            <MenuItem
                x:Name="Raspored_Button"
                Click="MenuItem_Click"
                Header="_Raspored"
                IsEnabled="False" />

            <MenuItem Click="MenuItem_Click_2" Header="_Demo" />
        </Menu>
