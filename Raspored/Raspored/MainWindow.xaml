                                    x:Name="dgrMainUcionica"
                                    Grid.Row="1"       
                                    AutoGenerateColumns="False"
                                    BorderThickness="1"
                                    IsReadOnly="True"
                                    IsSynchronizedWithCurrentItem="True"
