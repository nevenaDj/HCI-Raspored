            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            DockPanel.Dock="Bottom"
            Visibility="Visible">
            <Grid.RowDefinitions>
                <RowDefinition />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
            </Grid.ColumnDefinitions>

            <Grid
                x:Name="Prozor1"
                Grid.Row="0"
                Grid.Column="0">

                <Grid.RowDefinitions>
                    <RowDefinition Height="30" />
                    <RowDefinition Height="100" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="130" />
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>


                <StackPanel
                    Grid.Row="1"
                    Grid.Column="0"
                    Margin="10,0,0,0">
                    <RadioButton
                        Click="Ucionice_Radio"
                        Content="Učionice"
                        IsChecked="True" />
                    <RadioButton Click="Dani_Radio" Content="Dani" />
                </StackPanel>

                <Grid
                    Grid.Row="2"
                    Grid.RowSpan="2"
                    Grid.Column="0">

                    <ListView
                        x:Name="lsUcionice"
                        Grid.Column="0"
                        Padding="20,2,20,2"
                        ItemsSource="{Binding Ucionice}"
                        SelectionChanged="lsUcionice_SelectionChanged">
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <WrapPanel>
                                    <TextBlock Text="{Binding Oznaka}" />

                                </WrapPanel>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>

                    <ListView
                        x:Name="lsDani"
                        Grid.Column="0"
                        Padding="20,2,20,2"
                        ItemsSource="{Binding Dani}"
                        SelectionChanged="lsDani_SelectionChanged"
                        Visibility="Collapsed">
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <WrapPanel>
                                    <TextBlock Text="{Binding}" />

                                </WrapPanel>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </Grid>


