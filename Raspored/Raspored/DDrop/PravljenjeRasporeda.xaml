<Window
    x:Class="Raspored.DDrop.PravljenjeRasporeda"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Raspored.DDrop"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:model="clr-namespace:Raspored.Model"
    Title="PravljenjeRasporeda"
    Width="1000"
    Height="730"
    MinWidth="900"
    MinHeight="730"
    Closing="DataWindow_Closing"
    FontSize="16"
    Top="0"
    mc:Ignorable="d">
    <Window.Resources>
        <BitmapImage x:Key="FolderOpen" UriSource="/Raspored;component/foldero.png" />
        <BitmapImage x:Key="Bin" UriSource="/Raspored;component/bin.png" />
        <BitmapImage x:Key="FolderClosed" UriSource="/Raspored;component/folder.png" />
        <Style x:Key="SmallIcon" TargetType="Image">
            <Setter Property="MaxWidth" Value="16" />
            <Setter Property="MaxHeight" Value="16" />
            <Setter Property="Margin" Value="1" />
        </Style>

    </Window.Resources>
    <Grid>
        <Grid x:Name="Raspored1">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" MinWidth="200" />
                <ColumnDefinition Width="2" />
                <ColumnDefinition Width="*" MinWidth="600" />
            </Grid.ColumnDefinitions>
            <TextBlock
                Grid.Row="0"
                Grid.Column="0"
                Margin="5"
                HorizontalAlignment="Center"
                Foreground="DarkBlue">
                Prikaz predmeta po smerovima
            </TextBlock>

            <Button Grid.Row="1" Grid.Column="0">
                tekst
            </Button>
            <TreeView
                Name="trvPredmeti"
                Grid.Row="1"
                Grid.RowSpan="2"
                Grid.Column="0"
                Padding="3"
                BorderThickness="1"
                ItemsSource="{Binding Smerovi}"
                SelectedItemChanged="trvPredmeti_SelectedItemChanged">
                <TreeView.Resources>
                    <HierarchicalDataTemplate DataType="{x:Type model:Smer}" ItemsSource="{Binding Predmeti}">
                        <StackPanel Orientation="Horizontal">
                            <Image Margin="0,0,5,0">
                                <Image.Style>
                                    <Style BasedOn="{StaticResource ResourceKey=SmallIcon}" TargetType="Image">
                                        <Setter Property="Source" Value="{Binding Source={StaticResource ResourceKey=FolderClosed}, Mode=OneTime}" />
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType={x:Type TreeViewItem}}, Path=IsExpanded}" Value="True">
                                                <Setter Property="Source" Value="{Binding Source={StaticResource ResourceKey=FolderOpen}, Mode=OneTime}" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Image.Style>
                            </Image>
                            <TextBlock Text="{Binding Naziv}" />
                        </StackPanel>
                    </HierarchicalDataTemplate>
                    <DataTemplate DataType="{x:Type model:Predmet}">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock FontWeight="Bold" Text="{Binding Naziv}" />
                            <TextBlock Text=" (" />
                            <TextBlock Text="{Binding Oznaka}" />
                            <TextBlock Text=" " />
                            <TextBlock Text="{Binding Skracenica}" />
                            <TextBlock Text=")" />
                        </StackPanel>
                    </DataTemplate>
                </TreeView.Resources>
            </TreeView>

            <GridSplitter
                Grid.Row="1"
                Grid.RowSpan="2"
                Grid.Column="1"
                Width="2"
                HorizontalAlignment="Stretch"
                Background="#FFBCBCBC" />
            <ContentPresenter
                Grid.Row="1"
                Grid.Column="2"
                Content="{Binding ElementName=trvPredmeti, Path=SelectedItem}">
                <ContentPresenter.Resources>
                    <DataTemplate DataType="{x:Type model:Predmet}">
                        <Grid>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="50,50,50,0"
                                HorizontalAlignment="Center"
                                Foreground="Blue">
                                Izabrani predmet
                            </TextBlock>

                            <Border
                                Grid.Row="1"
                                Grid.Column="0"
                                Height="Auto"
                                Margin="50,0,50,50"
                                Padding="20"
                                Background="GhostWhite"
                                BorderBrush="Gainsboro"
                                BorderThickness="4"
                                CornerRadius="8,8,3,3">
                                <Grid Height="Auto">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>


                                    <TextBlock
                                        Grid.Row="1"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Oznaka
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="2"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Naziv
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Skracenica
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="4"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Opis
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="5"
                                        Grid.Column="0"
                                        Margin="5,20,2,2"
                                        Foreground="Blue">
                                        Broj termina
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="6"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Duzina termina
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="7"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Velicina grupe
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="8"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Operativni sistem
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="5"
                                        Grid.Column="2"
                                        Margin="5,20,2,2"
                                        Foreground="Blue">
                                        Projektor
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="6"
                                        Grid.Column="2"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Tabla
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="7"
                                        Grid.Column="2"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Pametna tabla
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="3"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Oznaka}" />
                                    <TextBlock
                                        Grid.Row="2"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="3"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Naziv}" />
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="3"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Skracenica}" />
                                    <TextBlock
                                        Grid.Row="4"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="3"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Opis}" />
                                    <TextBlock
                                        Grid.Row="5"
                                        Grid.Column="1"
                                        Margin="5,20,2,2"
                                        Text="{Binding Path=BrojTermina}" />
                                    <TextBlock
                                        Grid.Row="6"
                                        Grid.Column="1"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=DuzinaTermina}" />
                                    <TextBlock
                                        Grid.Row="7"
                                        Grid.Column="1"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=VelicinaGrupe}" />
                                    <TextBlock
                                        Grid.Row="8"
                                        Grid.Column="1"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Sistem}" />
                                    <TextBlock
                                        Grid.Row="5"
                                        Grid.Column="3"
                                        Margin="5,20,2,2"
                                        Text="{Binding Path=Projektor}" />
                                    <TextBlock
                                        Grid.Row="6"
                                        Grid.Column="3"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Tabla}" />
                                    <TextBlock
                                        Grid.Row="7"
                                        Grid.Column="3"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=PametnaTabla}" />

                                </Grid>
                            </Border>
                        </Grid>
                    </DataTemplate>
                    <DataTemplate DataType="{x:Type model:Smer}">
                        <Grid>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="50,50,50,0"
                                HorizontalAlignment="Center"
                                Foreground="DarkSlateBlue">
                                Podaci o smeru
                            </TextBlock>

                            <Border
                                Grid.Row="1"
                                Grid.Column="0"
                                Height="Auto"
                                Margin="50,0,50,50"
                                Padding="20"
                                Background="GhostWhite"
                                BorderBrush="AliceBlue"
                                BorderThickness="4"
                                CornerRadius="8,8,3,3">
                                <Grid Height="Auto">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock
                                        Grid.Row="0"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="DarkSlateBlue">
                                        Oznaka
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="1"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="DarkSlateBlue">
                                        Naziv
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="2"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="DarkSlateBlue">
                                        Skracenica
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="DarkSlateBlue">
                                        Opis
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="0"
                                        Grid.Column="1"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Oznaka}" />
                                    <TextBlock
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Naziv}" />
                                    <TextBlock
                                        Grid.Row="2"
                                        Grid.Column="1"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Skracenica}" />
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="1"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Opis}" />
                                </Grid>
                            </Border>
                        </Grid>
                    </DataTemplate>
                </ContentPresenter.Resources>
            </ContentPresenter>
            <Button
                Grid.Row="2"
                Grid.Column="2"
                Width="100"
                Margin="15,15,15,15"
                HorizontalAlignment="Left"
                Click="Korak1_Bez_Predmeta_Click">
                Bez predmeta
            </Button>
            <Button
                Grid.Row="2"
                Grid.Column="2"
                Width="80"
                Margin="0,15,15,15"
                HorizontalAlignment="Right"
                Click="Korak1_Click"
                IsEnabled="{Binding Korak1Enable}">
                Dalje
            </Button>

        </Grid>

        <Grid x:Name="Raspored2" Visibility="Collapsed">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition />
                <RowDefinition />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition
                    Width="*"
                    MinWidth="200"
                    MaxWidth="300" />
                <ColumnDefinition Width="2" />
                <ColumnDefinition Width="*" MinWidth="500" />

            </Grid.ColumnDefinitions>
            <TextBlock
                Grid.Row="0"
                Grid.Column="0"
                Margin="5"
                HorizontalAlignment="Center"
                Foreground="DarkBlue">
                Prikaz ucionica
            </TextBlock>

            <ListView
                x:Name="lsUcionice"
                Grid.Row="1"
                Grid.RowSpan="3"
                Grid.Column="0"
                Padding="20,2,20,2"
                ItemsSource="{Binding Ucionice}"
                SelectionChanged="lsUcionice_SelectionChanged">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <WrapPanel>
                            <TextBlock Text="{Binding Oznaka}" />

                        </WrapPanel>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
            <GridSplitter
                Grid.Row="1"
                Grid.RowSpan="3"
                Grid.Column="1"
                Width="2"
                HorizontalAlignment="Stretch"
                Background="#FFBCBCBC" />

            <ContentPresenter
                Grid.Row="1"
                Grid.Column="2"
                Content="{Binding ElementName=lsUcionice, Path=SelectedItem}">
                <ContentPresenter.Resources>
                    <DataTemplate DataType="{x:Type model:Ucionica}">
                        <Grid>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <TextBlock
                                Grid.Row="0"
                                Grid.Column="0"
                                Margin="50,10,50,0"
                                HorizontalAlignment="Center"
                                Foreground="Blue">
                                Izabrana ucionica
                            </TextBlock>

                            <Border
                                Grid.Row="1"
                                Grid.Column="0"
                                Height="Auto"
                                Margin="50,0,50,10"
                                Padding="20"
                                Background="GhostWhite"
                                BorderBrush="Gainsboro"
                                BorderThickness="4"
                                CornerRadius="8,8,3,3">
                                <Grid Height="Auto">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition />
                                        <ColumnDefinition Width="Auto" />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock
                                        Grid.Row="0"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Oznaka
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="1"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Opis
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Broj radnih mesta
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="4"
                                        Grid.Column="0"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Operativni sistem
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="2"
                                        Grid.Column="2"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Projektor
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="2"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Tabla
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="4"
                                        Grid.Column="2"
                                        Margin="5,2,2,2"
                                        Foreground="Blue">
                                        Pametna tabla
                                    </TextBlock>
                                    <TextBlock
                                        Grid.Row="0"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="3"
                                        Margin="-60,2,2,2"
                                        Text="{Binding Path=Oznaka}" />
                                    <TextBlock
                                        Grid.Row="1"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="3"
                                        Margin="-60,2,2,2"
                                        Text="{Binding Path=Opis}" />
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="1"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=BrojRadnihMesta}" />
                                    <TextBlock
                                        Grid.Row="4"
                                        Grid.Column="1"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Sistem}" />
                                    <TextBlock
                                        Grid.Row="2"
                                        Grid.Column="3"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Projektor}" />
                                    <TextBlock
                                        Grid.Row="3"
                                        Grid.Column="3"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=Tabla}" />
                                    <TextBlock
                                        Grid.Row="4"
                                        Grid.Column="3"
                                        Margin="5,2,2,2"
                                        Text="{Binding Path=PametnaTabla}" />

                                </Grid>
                            </Border>
                        </Grid>
                    </DataTemplate>
                </ContentPresenter.Resources>
            </ContentPresenter>

            <Grid Grid.Row="2" Grid.Column="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Grid.Column="0"
                    Margin="50,10,50,0"
                    HorizontalAlignment="Center"
                    Foreground="Blue">
                    Izabrani predmet
                </TextBlock>
                <Border
                    Grid.Row="1"
                    Grid.Column="0"
                    Height="Auto"
                    Margin="50,0,50,10"
                    Padding="20"
                    Background="GhostWhite"
                    BorderBrush="Gainsboro"
                    BorderThickness="4"
                    CornerRadius="8,8,3,3">
                    <Grid Height="Auto" DataContext="{Binding Path=SelectedPredmet, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                            <RowDefinition Height="Auto" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition />
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition />

                        </Grid.ColumnDefinitions>

                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="0"
                            Margin="5,2,2,2"
                            Foreground="Blue">
                            Oznaka
                        </TextBlock>
                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="0"
                            Margin="5,2,2,2"
                            Foreground="Blue">
                            Naziv
                        </TextBlock>
                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="0"
                            Margin="5,2,2,2"
                            Foreground="Blue">
                            Skracenica
                        </TextBlock>
                        <TextBlock
                            Grid.Row="4"
                            Grid.Column="0"
                            Margin="5,2,2,2"
                            Foreground="Blue">
                            Opis
                        </TextBlock>
                        <TextBlock
                            Grid.Row="5"
                            Grid.Column="0"
                            Margin="5,20,2,2"
                            Foreground="Blue">
                            Broj termina
                        </TextBlock>
                        <TextBlock
                            Grid.Row="6"
                            Grid.Column="0"
                            Margin="5,2,2,2"
                            Foreground="Blue">
                            Duzina termina
                        </TextBlock>
                        <TextBlock
                            Grid.Row="7"
                            Grid.Column="0"
                            Margin="5,2,2,2"
                            Foreground="Blue">
                            Velicina grupe
                        </TextBlock>
                        <TextBlock
                            Grid.Row="8"
                            Grid.Column="0"
                            Margin="5,2,2,2"
                            Foreground="Blue">
                            Operativni sistem
                        </TextBlock>
                        <TextBlock
                            Grid.Row="5"
                            Grid.Column="2"
                            Margin="5,20,2,2"
                            Foreground="Blue">
                            Projektor
                        </TextBlock>
                        <TextBlock
                            Grid.Row="6"
                            Grid.Column="2"
                            Margin="5,2,2,2"
                            Foreground="Blue">
                            Tabla
                        </TextBlock>
                        <TextBlock
                            Grid.Row="7"
                            Grid.Column="2"
                            Margin="5,2,2,2"
                            Foreground="Blue">
                            Pametna tabla
                        </TextBlock>
                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="1"
                            Grid.ColumnSpan="3"
                            Margin="5,2,2,2"
                            Text="{Binding Path=Oznaka}" />
                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="1"
                            Grid.ColumnSpan="3"
                            Margin="5,2,2,2"
                            Text="{Binding Path=Naziv}" />
                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="1"
                            Grid.ColumnSpan="3"
                            Margin="5,2,2,2"
                            Text="{Binding Path=Skracenica}" />
                        <TextBlock
                            Grid.Row="4"
                            Grid.Column="1"
                            Grid.ColumnSpan="3"
                            Margin="5,2,2,2"
                            Text="{Binding Path=Opis}" />
                        <TextBlock
                            Grid.Row="5"
                            Grid.Column="1"
                            Margin="5,20,2,2"
                            Text="{Binding Path=BrojTermina}" />
                        <TextBlock
                            Grid.Row="6"
                            Grid.Column="1"
                            Margin="5,2,2,2"
                            Text="{Binding Path=DuzinaTermina}" />
                        <TextBlock
                            Grid.Row="7"
                            Grid.Column="1"
                            Margin="5,2,2,2"
                            Text="{Binding Path=VelicinaGrupe}" />
                        <TextBlock
                            Grid.Row="8"
                            Grid.Column="1"
                            Margin="5,2,2,2"
                            Text="{Binding Path=Sistem}" />
                        <TextBlock
                            Grid.Row="5"
                            Grid.Column="3"
                            Margin="5,20,2,2"
                            Text="{Binding Path=Projektor}" />
                        <TextBlock
                            Grid.Row="6"
                            Grid.Column="3"
                            Margin="5,2,2,2"
                            Text="{Binding Path=Tabla}" />
                        <TextBlock
                            Grid.Row="7"
                            Grid.Column="3"
                            Margin="5,2,2,2"
                            Text="{Binding Path=PametnaTabla}" />

                    </Grid>
                </Border>
            </Grid>
            <Button
                Grid.Row="3"
                Grid.Column="2"
                Width="80"
                Margin="0,15,100,15"
                HorizontalAlignment="Right"
                Click="Korak_Nazad_Click"
                IsEnabled="True">
                Nazad
            </Button>
            <Button
                Grid.Row="3"
                Grid.Column="2"
                Width="80"
                Margin="0,15,15,15"
                HorizontalAlignment="Right"
                Click="Korak2_Click"
                IsEnabled="{Binding Korak2Enable}">
                Dalje
            </Button>
        </Grid>

        <Grid x:Name="Raspored3" Visibility="Collapsed">
            <Grid.RowDefinitions>
                <RowDefinition Height="30" />
                <RowDefinition />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="130" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>

            <Grid
                Grid.Row="0"
                Grid.RowSpan="2"
